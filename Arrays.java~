


pubic class Arrays{
  
  int[] numbers;
  
  public Arrays() {
    
    int[] numbers = new int[5];
    
  }
  
  public void doStuff(){
    
    int i = 0;
    
    while(i < numbers.length) {
      
      numbers[i] = (int)(Math.pow(2,i) );
      
      i++;
      
    }
    
  }
  
  public static void main (String[] args){
    
    Arrays list = new Arrays();
    list.doStuff();
    
  }
  
}
    