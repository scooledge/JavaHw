


public class ButtonButton {
  
  private NscWindow win;
  
  private NscEllipse oval;
  
  public ButtonButton() {
    
    win = new NsccWindow();
    
    win.setTitle("ButtonButton");
    
    oval = new NsccEllispe(100,70,200,250);
    
    oval.setFilled(true);
    
    oval.setBackground(java.awt.Color.blue);
    
    win.add(oval);
    
    win.repaint();
    
  }